<androidx.coordinatorlayout.widget.CoordinatorLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<com.google.android.material.appbar.AppBarLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:theme="@style/AppTheme.AppBarOverlay">
		<androidx.appcompat.widget.Toolbar
			android:id="@+id/_toolbar"
			android:layout_width="match_parent"
			android:layout_height="?attr/actionBarSize"
			android:background="?attr/colorPrimary"
			app:popupTheme="@style/AppTheme.PopupOverlay"/>
	</com.google.android.material.appbar.AppBarLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		app:layout_behavior="@string/appbar_scrolling_view_behavior">
		<ScrollView
			android:id="@+id/vscroll1"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="8dp">
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear4"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview2"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Project Name: "
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"/>
					<EditText
						android:id="@+id/edittext_pname"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:textSize="16sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear5"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Author: "
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"/>
					<EditText
						android:id="@+id/edittext_author"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:textSize="16sp"
						android:textColor="#000000"
						android:textColorHint="#607D8B"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear8"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview4"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Build Type: "
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"/>
					<Spinner
						android:id="@+id/spinner2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:spinnerMode="dropdown"/>
				</LinearLayout>
				<Switch
					android:id="@+id/switch2"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_vertical"
					android:text="Advance Setting"
					android:textSize="14sp"
					android:textStyle="bold"
					android:textColor="#000000"/>
				<LinearLayout
					android:id="@+id/linear_advan"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="8dp"
					android:orientation="vertical">
					<TextView
						android:id="@+id/textview5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="File"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"/>
					<Switch
						android:id="@+id/switch_index"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Index.html"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_log"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Log.kode"
						android:textSize="14sp"
						android:textColor="#000000"
						android:enabled="false"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_con"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Config.st"
						android:textSize="14sp"
						android:textColor="#000000"
						android:enabled="false"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_readme"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Readme.md"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_pinfo"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Project.info"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_license"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="License.txt"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_help"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Help.md"
						android:textSize="14sp"
						android:textColor="#000000"/>
					<TextView
						android:id="@+id/textview6"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Folder"
						android:textSize="14sp"
						android:textStyle="bold"
						android:textColor="#000000"/>
					<Switch
						android:id="@+id/switch_images"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Images"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_font"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Font"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_media"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Media"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_res"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Res"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_db"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="Database"
						android:textSize="14sp"
						android:textColor="#000000"/>
					<Switch
						android:id="@+id/switch_css"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="CSS"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
					<Switch
						android:id="@+id/switch_js"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:gravity="center_vertical"
						android:text="JavaScript"
						android:textSize="14sp"
						android:textColor="#000000"
						android:checked="true"/>
				</LinearLayout>
				<Button
					android:id="@+id/button_create"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Create"
					android:textSize="16sp"
					android:textColor="#000000"/>
			</LinearLayout>
		</ScrollView>
	</LinearLayout>
</androidx.coordinatorlayout.widget.CoordinatorLayout>
